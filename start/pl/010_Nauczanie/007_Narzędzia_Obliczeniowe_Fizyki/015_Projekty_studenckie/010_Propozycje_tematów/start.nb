      ")"}]}], " ", "\[TildeTilde]", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["\[Delta]", "2"]], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["u", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"i", " ", "+", " ", "1"}], ")"}], "j"}]], " ", "+", 
       SubscriptBox["u", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"i", " ", "-", " ", "1"}], ")"}], "j"}]], " ", "+", " ", 
       SubscriptBox["u", 
        RowBox[{"i", " ", 
         RowBox[{"(", 
          RowBox[{"j", " ", "+", " ", "1"}], ")"}]}]], "+", 
